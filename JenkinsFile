pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build'){
            steps{
                echo "Building Job"
            }
        }
        stage('Deploy'){
            steps{
                echo "Deploying Job"
            }
        }
        stage('Release'){
            steps{
                echo "Releasing jobs"
            }
        }
    }
}
